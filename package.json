{
  "private": true,
  "engines": {
    "node": "6.11.1"
  },
  "nyc": {
    "sourceMap": false,
    "instrument": false
  },
  "config": {
    "exec_test": "./node_modules/.bin/mocha --compilers js:babel-register"
  },
  "scripts": {
    "start": "./node_modules/.bin/gulp",
    "es5": "node src-es5/server",
    "test": "npm run test-server && npm run test-client && npm run test-browser",
    "test-one": "$npm_package_config_exec_test",
    "test-server": "$npm_package_config_exec_test test/index.js",
    "test-client": "$npm_package_config_exec_test test/client/index.js",
    "test-browser": "$npm_package_config_exec_test test/browser/index.js",
    "cover": "./cover",
    "autotest": "./node_modules/.bin/gulp autotest",
    "deploy": "./node_modules/.bin/gulp deploy"
  },
  "standard": {
    "parser": "babel-eslint",
    "global": [
      "after",
      "afterEach",
      "before",
      "beforeEach",
      "describe",
      "expect",
      "it",
      "spy"
    ]
  },
